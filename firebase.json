{
 "hosting": {
  "public": "dist",
  "headers": [ {
   "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
   "headers": [ {
    "key": "Access-Control-Allow-Origin",
    "value": "*"
  }]}, {
   "source" : "**/*.@(css|js|json|png|ico)",
   "headers" : [ {
    "key" : "Cache-Control",
    "value" : "max-age=31536000"
   }]
  }],
  "cleanUrls": true,
  "trailingSlash": false
 }
}